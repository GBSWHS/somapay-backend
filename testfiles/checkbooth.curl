# 어드민 로그인
curl -X POST http://localhost:8443/login \
  -H "Content-Type: application/json" \
  -d '{"student_number":"admin","password":"admin"}'

// 관리자가 부스 생성
curl -X POST http://localhost:8443/booths \
  -H "Content-Type: application/json" \
  -H "Authorization: {{ADMIN UUID}}" \
  -d '{"name":"Booth A","user_id":4}'

// 일반 유저가 부스 조회
curl -X GET http://localhost:8443/booths/1 \
  -H "Authorization: {{USER UUID}}"

// 호스트가 부스 조회
curl -X GET http://localhost:8443/booths/1 \
  -H "Authorization: {{HOST UUID}}"
